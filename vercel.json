{
    "version": 2,
    "routes": [
      {
        "src": "/health",
        "dest": "server.js"
      },
      {
        "src": "/api/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/uploads/(.*)",
        "dest": "server.js"
      },
      {
        "src": "/(.*)",
        "dest": "server.js"
      }
    ],
    "functions": {
      "server.js": {
        "maxDuration": 30,
        "memory": 1024
      }
    }
  }